{
  "name": "maintenance-tr-client",
  "version": "1.0.0",
  "description": "Maintenance tracker client",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage -u",
    "prestart": "babel server.js -d dist",
    "start": "node ./dist/server.js",
    "dev": "webpack-dev-server --mode development --open",
    "lint": "eslint --ext .js --ext .jsx src",
    "build": "webpack --mode production",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/Client/tests/testSetup.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "src"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 30,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klevamane/Maintenance-tr-client.git"
  },
  "keywords": [
    "React",
    "Redux"
  ],
  "author": "Onengiye Richard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/klevamane/Maintenance-tr-client/issues"
  },
  "homepage": "https://github.com/klevamane/Maintenance-tr-client#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-loader": "^7.1.5",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jwt-decode": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-moment": "^0.8.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "redux-devtools-extension": "^2.13.5",
    "sinon": "^6.3.5",
    "url-loader": "^1.1.1",
    "webpack-dev-server": "^3.1.5"
  }
}
